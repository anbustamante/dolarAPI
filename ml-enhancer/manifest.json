{
  "manifest_version": 2,
  "name": "ML-Enhancer",
  "version": "1.0",
  "description": "Add text and a button to price grid containers",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["app.js"]
    }
  ],
  "icons": {
    "48": "icon.png"
  }
}
